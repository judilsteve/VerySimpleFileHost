{
  "private": true,
  "name": "vsfh-client",
  "version": "0.1.0",
  "scripts": {
    "build": "preact build --no-esm --no-sw --prerenderUrls ./prerender-urls.js --no-json --preload",
    "start": "preact watch",
    "lint": "eslint src",
    "generate-api-client": "SET JAVA_OPTS=-Dio.swagger.v3.parser.util.RemoteUrl.trustAll=true && openapi-generator-cli generate -i https://localhost:7270/swagger/v1/swagger.json  -g typescript-fetch --additional-properties=withoutRuntimeChecks=true -o ./src/API"
  },
  "eslintConfig": {
    "extends": "preact"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@openapitools/openapi-generator-cli": "^2.4.18",
    "@preact/cli-plugin-less": "^0.0.4-alpha.0",
    "@types/react": "^17.0.45",
    "eslint": "^7.17.0",
    "eslint-config-preact": "^1.1.3",
    "preact-cli": "^3.0.0",
    "sirv-cli": "^1.0.3"
  },
  "dependencies": {
    "@fontsource/lato": "^4.5.5",
    "@types/zxcvbn": "^4.4.1",
    "preact": "^10.1.0",
    "preact-async-route": "^2.2.1",
    "preact-render-to-string": "^5.1.2",
    "preact-router": "^4.0.1",
    "semantic-ui-less": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "zxcvbn": "^4.4.2"
  }
}
